  "dataSources": {
    "ds_xYnkNKo0": {
      "type": "ds.search",
      "options": {
        "query": "source=\"ssh_logs.json\" host=\"LinuxServer\" sourcetype=\"_json\"\n | stats count AS \"Total SSH Events\"",
        "queryParameters": {
          "earliest": "$time_range.earliest$",
          "latest": "$time_range.latest$"
        }
      },
      "name": "Total SSH Events search"
    },
    "ds_fKVF7dMV": {
      "type": "ds.search",
      "options": {
        "query": "source=\"ssh_logs.json\" host=\"LinuxServer\" sourcetype=\"_json\" event_type=\"Successful SSH Login\" \n| stats count AS \"Successful Logins\"",
        "queryParameters": {
          "earliest": "$time_range.earliest$",
          "latest": "$time_range.latest$"
        }
      },
      "name": "Successful Logins search"
    },
    "ds_8m0GeOOP": {
      "type": "ds.search",
      "options": {
        "query": "source=\"ssh_logs.json\" host=\"LinuxServer\" sourcetype=\"_json\" event_type=\"Failed SSH Login\"\n| stats count AS \"Failed Login\"",
        "queryParameters": {
          "earliest": "$time_range.earliest$",
          "latest": "$time_range.latest$"
        }
      },
      "name": "Failed Logins search"
    },
    "ds_qJ7R4Hzb": {
      "type": "ds.search",
      "options": {
        "query": "source=\"ssh_logs.json\" host=\"LinuxServer\" sourcetype=\"_json\" event_type=\"Connection without Authentication\"\n| stats count AS \"Connection without Authentication\"",
        "queryParameters": {
          "earliest": "$time_range.earliest$",
          "latest": "$time_range.latest$"
        }
      },
      "name": "Connection without Authentication search"
    },
    "ds_weYxwpV6": {
      "type": "ds.search",
      "options": {
        "query": "source=\"ssh_logs_new.json\" host=\"LinuxServer\" sourcetype=\"_json\" event_type=\"Failed SSH Login\" | top username",
        "queryParameters": {
          "earliest": "$time_range.earliest$",
          "latest": "$time_range.latest$"
        }
      },
      "name": "Failed Logins by username search"
    },
    "ds_3hONiRXu": {
      "type": "ds.search",
      "options": {
        "query": "source=\"ssh_logs_new.json\" host=\"LinuxServer\" sourcetype=\"_json\" event_type=\"Multiple Failed Authentication Attempts\" | top id.orig_h",
        "queryParameters": {
          "earliest": "$time_range.earliest$",
          "latest": "$time_range.latest$"
        }
      },
      "name": "Possible Brute Force b IP Address search"
    },
    "ds_LzlbipJC": {
      "type": "ds.search",
      "options": {
        "query": "source=\"ssh_logs_new.json\" host=\"LinuxServer\" sourcetype=\"_json\" event_type=\"Multiple Failed Authentication Attempts\" \n| table id.orig_h\n| iplocation id.orig_h\n| stats count by Country\n| geom geo_countries featureIdField=\"Country\"",
        "queryParameters": {
          "earliest": "$time_range.earliest$",
          "latest": "$time_range.latest$"
        }
      },
      "name": "Brute Force attack with geo-location search"
    }
  },
